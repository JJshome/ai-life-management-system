<svg width="700" height="450" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a8f" stop-opacity="0.1"/>
      <stop offset="100%" stop-color="#42b0d1" stop-opacity="0.1"/>
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="2" dy="2" stdDeviation="3" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Background rectangle -->
  <rect x="0" y="0" width="700" height="450" fill="url(#bg-gradient)" rx="10" ry="10"/>
  
  <!-- Title -->
  <text x="350" y="50" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#2c3e50" text-anchor="middle">Value Proposition for Insurance Companies</text>
  
  <!-- Insurance company building -->
  <g transform="translate(100, 180)" filter="url(#shadow)">
    <rect x="-60" y="-80" width="120" height="160" rx="5" fill="#3498db"/>
    <rect x="-50" y="-70" width="100" height="150" rx="3" fill="#ffffff" opacity="0.3"/>
    
    <!-- Windows -->
    <rect x="-40" y="-60" width="20" height="20" rx="2" fill="#ffffff"/>
    <rect x="-10" y="-60" width="20" height="20" rx="2" fill="#ffffff"/>
    <rect x="20" y="-60" width="20" height="20" rx="2" fill="#ffffff"/>
    
    <rect x="-40" y="-30" width="20" height="20" rx="2" fill="#ffffff"/>
    <rect x="-10" y="-30" width="20" height="20" rx="2" fill="#ffffff"/>
    <rect x="20" y="-30" width="20" height="20" rx="2" fill="#ffffff"/>
    
    <rect x="-40" y="0" width="20" height="20" rx="2" fill="#ffffff"/>
    <rect x="-10" y="0" width="20" height="20" rx="2" fill="#ffffff"/>
    <rect x="20" y="0" width="20" height="20" rx="2" fill="#ffffff"/>
    
    <!-- Door -->
    <rect x="-10" y="40" width="20" height="40" rx="2" fill="#ffffff"/>
    
    <!-- Roof -->
    <path d="M-70,-80 L0,-120 L70,-80 Z" fill="#2980b9"/>
    
    <!-- Insurance Sign -->
    <rect x="-40" y="-100" width="80" height="20" rx="5" fill="#ffffff"/>
    <text x="0" y="-85" font-family="Arial, sans-serif" font-size="12" fill="#2980b9" text-anchor="middle">INSURANCE</text>
  </g>
  
  <!-- AI System -->
  <g transform="translate(600, 180)" filter="url(#shadow)">
    <rect x="-60" y="-60" width="120" height="120" rx="10" fill="#9b59b6"/>
    
    <!-- System Screen -->
    <rect x="-50" y="-50" width="100" height="70" rx="5" fill="#ffffff"/>
    
    <!-- Screen Content -->
    <line x1="-40" y1="-30" x2="-20" y2="-30" stroke="#9b59b6" stroke-width="2"/>
    <line x1="-40" y1="-20" x2="-10" y2="-20" stroke="#9b59b6" stroke-width="2"/>
    <line x1="-40" y1="-10" x2="-30" y2="-10" stroke="#9b59b6" stroke-width="2"/>
    
    <circle cx="20" cy="-20" r="20" fill="none" stroke="#9b59b6" stroke-width="2"/>
    <path d="M10,-30 L20,-20 L30,-40" stroke="#9b59b6" stroke-width="2" fill="none"/>
    
    <!-- Keyboard/Input -->
    <rect x="-40" y="30" width="80" height="20" rx="5" fill="#ffffff"/>
    <circle cx="-30" cy="40" r="5" fill="#9b59b6"/>
    <circle cx="-15" cy="40" r="5" fill="#9b59b6"/>
    <circle cx="0" cy="40" r="5" fill="#9b59b6"/>
    <circle cx="15" cy="40" r="5" fill="#9b59b6"/>
    <circle cx="30" cy="40" r="5" fill="#9b59b6"/>
    
    <!-- AI System Label -->
    <rect x="-50" y="60" width="100" height="20" rx="5" fill="#ffffff"/>
    <text x="0" y="75" font-family="Arial, sans-serif" font-size="12" fill="#9b59b6" text-anchor="middle">AI LIFE SYSTEM</text>
  </g>
  
  <!-- Center benefits area -->
  <g transform="translate(350, 180)">
    <rect x="-150" y="-120" width="300" height="240" rx="10" fill="#ffffff" opacity="0.2" filter="url(#shadow)"/>
    
    <!-- Benefit 1: Improved Underwriting -->
    <g transform="translate(0, -90)">
      <circle cx="-120" cy="0" r="15" fill="#e74c3c"/>
      <text x="-120" y="5" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">1</text>
      <text x="-20" y="5" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50" text-anchor="middle">Improved Underwriting</text>
      <line x1="80" y1="0" x2="100" y2="0" stroke="#e74c3c" stroke-width="2" stroke-dasharray="2,2"/>
      <circle cx="110" cy="0" r="10" fill="#e74c3c"/>
      <text x="110" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">+</text>
    </g>
    
    <!-- Benefit 2: Reduced Premiums -->
    <g transform="translate(0, -50)">
      <circle cx="-120" cy="0" r="15" fill="#f39c12"/>
      <text x="-120" y="5" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">2</text>
      <text x="-20" y="5" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50" text-anchor="middle">Reduced Premiums</text>
      <line x1="80" y1="0" x2="100" y2="0" stroke="#f39c12" stroke-width="2" stroke-dasharray="2,2"/>
      <circle cx="110" cy="0" r="10" fill="#f39c12"/>
      <text x="110" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">$</text>
    </g>
    
    <!-- Benefit 3: Personalized Products -->
    <g transform="translate(0, -10)">
      <circle cx="-120" cy="0" r="15" fill="#2ecc71"/>
      <text x="-120" y="5" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">3</text>
      <text x="-20" y="5" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50" text-anchor="middle">Personalized Products</text>
      <line x1="80" y1="0" x2="100" y2="0" stroke="#2ecc71" stroke-width="2" stroke-dasharray="2,2"/>
      <circle cx="110" cy="0" r="10" fill="#2ecc71"/>
      <text x="110" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">â˜…</text>
    </g>
    
    <!-- Benefit 4: Preventive Interventions -->
    <g transform="translate(0, 30)">
      <circle cx="-120" cy="0" r="15" fill="#3498db"/>
      <text x="-120" y="5" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">4</text>
      <text x="-20" y="5" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50" text-anchor="middle">Preventive Interventions</text>
      <line x1="80" y1="0" x2="100" y2="0" stroke="#3498db" stroke-width="2" stroke-dasharray="2,2"/>
      <circle cx="110" cy="0" r="10" fill="#3498db"/>
      <text x="110" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">âš•</text>
    </g>
    
    <!-- Benefit 5: Data-Driven Insights -->
    <g transform="translate(0, 70)">
      <circle cx="-120" cy="0" r="15" fill="#9b59b6"/>
      <text x="-120" y="5" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">5</text>
      <text x="-20" y="5" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50" text-anchor="middle">Data-Driven Insights</text>
      <line x1="80" y1="0" x2="100" y2="0" stroke="#9b59b6" stroke-width="2" stroke-dasharray="2,2"/>
      <circle cx="110" cy="0" r="10" fill="#9b59b6"/>
      <text x="110" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">ðŸ“Š</text>
    </g>
  </g>
  
  <!-- Connection lines between Insurance and System -->
  <g>
    <!-- Main connection line -->
    <path d="M160,180 L580,180" stroke="#2c3e50" stroke-width="3" fill="none" stroke-dasharray="5,5" filter="url(#shadow)"/>
    
    <!-- Customer data flow -->
    <g transform="translate(350, 140)" filter="url(#shadow)">
      <rect x="-100" y="-20" width="200" height="40" rx="20" fill="#1abc9c"/>
      <text x="0" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Customer Health Data Flow</text>
      <!-- Data flow icons -->
      <circle cx="-70" cy="0" r="10" fill="#ffffff" opacity="0.6"/>
      <circle cx="-40" cy="0" r="10" fill="#ffffff" opacity="0.6"/>
      <circle cx="-10" cy="0" r="10" fill="#ffffff" opacity="0.6"/>
      <circle cx="20" cy="0" r="10" fill="#ffffff" opacity="0.6"/>
      <circle cx="50" cy="0" r="10" fill="#ffffff" opacity="0.6"/>
      <circle cx="80" cy="0" r="10" fill="#ffffff" opacity="0.6"/>
    </g>
    
    <!-- Results flow -->
    <g transform="translate(350, 220)" filter="url(#shadow)">
      <rect x="-100" y="-20" width="200" height="40" rx="20" fill="#e74c3c"/>
      <text x="0" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Predictive Results &amp; Insights</text>
      <!-- Results icons -->
      <path d="M-70,5 L-65,-5 L-60,0 L-55,-10 L-50,-5 L-45,0 L-40,5" stroke="#ffffff" stroke-width="1.5" fill="none"/>
      <path d="M-20,5 L-15,-5 L-10,0 L-5,-10 L0,-5 L5,0 L10,5" stroke="#ffffff" stroke-width="1.5" fill="none"/>
      <path d="M30,5 L35,-5 L40,0 L45,-10 L50,-5 L55,0 L60,5" stroke="#ffffff" stroke-width="1.5" fill="none"/>
    </g>
  </g>
  
  <!-- Bottom caption -->
  <g transform="translate(350, 380)">
    <rect x="-330" y="-15" width="660" height="30" rx="15" fill="#2c3e50" opacity="0.7"/>
    <text x="0" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">
      Reduce costs, improve risk assessment, and develop tailored insurance products
    </text>
  </g>
  
  <!-- People/customers at the bottom -->
  <g transform="translate(120, 340)" filter="url(#shadow)">
    <!-- Person 1 -->
    <circle cx="0" cy="0" r="15" fill="#e74c3c"/>
    <circle cx="-5" cy="-5" r="3" fill="#ffffff"/>
    <circle cx="5" cy="-5" r="3" fill="#ffffff"/>
    <path d="M-5,5 Q0,10 5,5" stroke="#ffffff" stroke-width="1.5" fill="none"/>
    <rect x="-10" y="15" width="20" height="25" rx="5" fill="#e74c3c"/>
  </g>
  
  <g transform="translate(180, 340)" filter="url(#shadow)">
    <!-- Person 2 -->
    <circle cx="0" cy="0" r="15" fill="#f39c12"/>
    <circle cx="-5" cy="-5" r="3" fill="#ffffff"/>
    <circle cx="5" cy="-5" r="3" fill="#ffffff"/>
    <path d="M-5,5 Q0,10 5,5" stroke="#ffffff" stroke-width="1.5" fill="none"/>
    <rect x="-10" y="15" width="20" height="25" rx="5" fill="#f39c12"/>
  </g>
  
  <g transform="translate(240, 340)" filter="url(#shadow)">
    <!-- Person 3 -->
    <circle cx="0" cy="0" r="15" fill="#2ecc71"/>
    <circle cx="-5" cy="-5" r="3" fill="#ffffff"/>
    <circle cx="5" cy="-5" r="3" fill="#ffffff"/>
    <path d="M-5,5 Q0,10 5,5" stroke="#ffffff" stroke-width="1.5" fill="none"/>
    <rect x="-10" y="15" width="20" height="25" rx="5" fill="#2ecc71"/>
  </g>
  
  <g transform="translate(520, 340)" filter="url(#shadow)">
    <!-- Person 4 - elderly -->
    <circle cx="0" cy="0" r="15" fill="#3498db"/>
    <circle cx="-5" cy="-5" r="3" fill="#ffffff"/>
    <circle cx="5" cy="-5" r="3" fill="#ffffff"/>
    <path d="M-5,5 Q0,8 5,5" stroke="#ffffff" stroke-width="1.5" fill="none"/>
    <rect x="-10" y="15" width="20" height="25" rx="5" fill="#3498db"/>
    <line x1="-5" y1="-15" x2="0" y2="-20" stroke="#ffffff" stroke-width="1.5"/>
    <line x1="5" y1="-15" x2="0" y2="-20" stroke="#ffffff" stroke-width="1.5"/>
  </g>
  
  <g transform="translate(580, 340)" filter="url(#shadow)">
    <!-- Person 5 - elderly -->
    <circle cx="0" cy="0" r="15" fill="#9b59b6"/>
    <circle cx="-5" cy="-5" r="3" fill="#ffffff"/>
    <circle cx="5" cy="-5" r="3" fill="#ffffff"/>
    <path d="M-5,5 Q0,8 5,5" stroke="#ffffff" stroke-width="1.5" fill="none"/>
    <rect x="-10" y="15" width="20" height="25" rx="5" fill="#9b59b6"/>
    <line x1="-5" y1="-15" x2="0" y2="-20" stroke="#ffffff" stroke-width="1.5"/>
    <line x1="5" y1="-15" x2="0" y2="-20" stroke="#ffffff" stroke-width="1.5"/>
  </g>
</svg>