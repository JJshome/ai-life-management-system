<svg width="600" height="500" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a8f" stop-opacity="0.1"/>
      <stop offset="100%" stop-color="#42b0d1" stop-opacity="0.1"/>
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="2" dy="2" stdDeviation="3" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Background rectangle -->
  <rect x="0" y="0" width="600" height="500" fill="url(#bg-gradient)" rx="10" ry="10"/>
  
  <!-- Title -->
  <text x="300" y="50" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#2c3e50" text-anchor="middle">Key Technologies</text>
  
  <!-- Center hexagon -->
  <g transform="translate(300, 250)" filter="url(#shadow)">
    <path d="M0,-70 L60.6,-35 L60.6,35 L0,70 L-60.6,35 L-60.6,-35 Z" fill="#2c3e50"/>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="18" fill="#ffffff" text-anchor="middle" dominant-baseline="middle">AI Life</text>
    <text x="0" y="25" font-family="Arial, sans-serif" font-size="18" fill="#ffffff" text-anchor="middle" dominant-baseline="middle">System</text>
  </g>
  
  <!-- Ultra-high-speed communication -->
  <g transform="translate(300, 100)" filter="url(#shadow)">
    <circle cx="0" cy="0" r="45" fill="#3498db"/>
    <text x="0" y="-10" font-family="Arial, sans-serif" font-size="12" fill="#ffffff" text-anchor="middle">Ultra-high-speed</text>
    <text x="0" y="10" font-family="Arial, sans-serif" font-size="12" fill="#ffffff" text-anchor="middle">Communication</text>
    
    <!-- Signal waves -->
    <path d="M-20,-20 Q-10,-30 0,-20 Q10,-30 20,-20" stroke="#ffffff" stroke-width="2" fill="none"/>
    <path d="M-25,-10 Q-12.5,-25 0,-10 Q12.5,-25 25,-10" stroke="#ffffff" stroke-width="2" fill="none"/>
    <path d="M-30,0 Q-15,-20 0,0 Q15,-20 30,0" stroke="#ffffff" stroke-width="2" fill="none"/>
  </g>
  
  <!-- Edge AI -->
  <g transform="translate(110, 180)" filter="url(#shadow)">
    <circle cx="0" cy="0" r="45" fill="#9b59b6"/>
    <text x="0" y="-5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Edge AI</text>
    
    <!-- AI chip cartoon -->
    <rect x="-20" y="5" width="40" height="20" rx="2" fill="#ffffff"/>
    <line x1="-15" y1="5" x2="-15" y2="25" stroke="#9b59b6" stroke-width="1"/>
    <line x1="-10" y1="5" x2="-10" y2="25" stroke="#9b59b6" stroke-width="1"/>
    <line x1="-5" y1="5" x2="-5" y2="25" stroke="#9b59b6" stroke-width="1"/>
    <line x1="0" y1="5" x2="0" y2="25" stroke="#9b59b6" stroke-width="1"/>
    <line x1="5" y1="5" x2="5" y2="25" stroke="#9b59b6" stroke-width="1"/>
    <line x1="10" y1="5" x2="10" y2="25" stroke="#9b59b6" stroke-width="1"/>
    <line x1="15" y1="5" x2="15" y2="25" stroke="#9b59b6" stroke-width="1"/>
  </g>
  
  <!-- Semiconductor Sensor Technology -->
  <g transform="translate(490, 180)" filter="url(#shadow)">
    <circle cx="0" cy="0" r="45" fill="#e74c3c"/>
    <text x="0" y="-15" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">Ultra-high-density</text>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">Semiconductor</text>
    <text x="0" y="15" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">Sensors</text>
    
    <!-- Sensor icon -->
    <circle cx="0" cy="-25" r="8" fill="#ffffff"/>
    <path d="M-8,15 L8,15 L4,25 L-4,25 Z" fill="#ffffff"/>
    <rect x="-3" y="-17" width="6" height="32" fill="#ffffff"/>
  </g>
  
  <!-- Electrochemical Impedance -->
  <g transform="translate(110, 320)" filter="url(#shadow)">
    <circle cx="0" cy="0" r="45" fill="#f39c12"/>
    <text x="0" y="-15" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">Electrochemical</text>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">Impedance</text>
    <text x="0" y="15" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">Measurement</text>
    
    <!-- Ear sensor with waves -->
    <path d="M-10,20 C-20,15 -20,-5 -10,-10 C0,-15 0,-5 10,-10 C20,-5 20,15 10,20 Z" stroke="#ffffff" stroke-width="2" fill="none"/>
    <path d="M-5,0 Q0,-10 5,0" stroke="#ffffff" stroke-width="1" stroke-dasharray="2,2"/>
    <path d="M-10,0 Q0,-15 10,0" stroke="#ffffff" stroke-width="1" stroke-dasharray="2,2"/>
    <path d="M-15,0 Q0,-20 15,0" stroke="#ffffff" stroke-width="1" stroke-dasharray="2,2"/>
  </g>
  
  <!-- Advanced AI Algorithms -->
  <g transform="translate(490, 320)" filter="url(#shadow)">
    <circle cx="0" cy="0" r="45" fill="#2ecc71"/>
    <text x="0" y="-15" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">Advanced AI</text>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">Algorithms</text>
    <text x="0" y="15" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">&amp; ML Models</text>
    
    <!-- Brain network icon -->
    <circle cx="-15" cy="-5" r="5" fill="#ffffff"/>
    <circle cx="0" cy="-15" r="5" fill="#ffffff"/>
    <circle cx="15" cy="-5" r="5" fill="#ffffff"/>
    <circle cx="-10" cy="15" r="5" fill="#ffffff"/>
    <circle cx="10" cy="15" r="5" fill="#ffffff"/>
    
    <line x1="-15" y1="-5" x2="0" y2="-15" stroke="#ffffff" stroke-width="1"/>
    <line x1="0" y1="-15" x2="15" y2="-5" stroke="#ffffff" stroke-width="1"/>
    <line x1="-15" y1="-5" x2="-10" y2="15" stroke="#ffffff" stroke-width="1"/>
    <line x1="-10" y1="15" x2="10" y2="15" stroke="#ffffff" stroke-width="1"/>
    <line x1="10" y1="15" x2="15" y2="-5" stroke="#ffffff" stroke-width="1"/>
    <line x1="-15" y1="-5" x2="15" y2="-5" stroke="#ffffff" stroke-width="1"/>
    <line x1="0" y1="-15" x2="-10" y2="15" stroke="#ffffff" stroke-width="1"/>
    <line x1="0" y1="-15" x2="10" y2="15" stroke="#ffffff" stroke-width="1"/>
  </g>
  
  <!-- Blockchain Technology -->
  <g transform="translate(210, 420)" filter="url(#shadow)">
    <circle cx="0" cy="0" r="45" fill="#1abc9c"/>
    <text x="0" y="-5" font-family="Arial, sans-serif" font-size="12" fill="#ffffff" text-anchor="middle">Blockchain</text>
    <text x="0" y="10" font-family="Arial, sans-serif" font-size="12" fill="#ffffff" text-anchor="middle">Technology</text>
    
    <!-- Chain blocks icon -->
    <rect x="-25" y="-25" width="15" height="15" rx="2" fill="#ffffff"/>
    <rect x="-5" y="-25" width="15" height="15" rx="2" fill="#ffffff"/>
    <rect x="15" y="-25" width="15" height="15" rx="2" fill="#ffffff"/>
    <rect x="-15" y="15" width="15" height="15" rx="2" fill="#ffffff"/>
    <rect x="5" y="15" width="15" height="15" rx="2" fill="#ffffff"/>
    
    <line x1="-17.5" y1="-25" x2="-17.5" y2="-30" stroke="#ffffff" stroke-width="1"/>
    <line x1="2.5" y1="-25" x2="2.5" y2="-30" stroke="#ffffff" stroke-width="1"/>
    <line x1="22.5" y1="-25" x2="22.5" y2="-30" stroke="#ffffff" stroke-width="1"/>
    <line x1="-7.5" y1="15" x2="-7.5" y2="10" stroke="#ffffff" stroke-width="1"/>
    <line x1="12.5" y1="15" x2="12.5" y2="10" stroke="#ffffff" stroke-width="1"/>
    
    <line x1="-17.5" y1="-30" x2="22.5" y2="-30" stroke="#ffffff" stroke-width="1"/>
    <line x1="-7.5" y1="10" x2="12.5" y2="10" stroke="#ffffff" stroke-width="1"/>
    <line x1="-17.5" y1="-10" x2="22.5" y2="-10" stroke="#ffffff" stroke-width="1"/>
    <line x1="2.5" y1="-10" x2="2.5" y2="10" stroke="#ffffff" stroke-width="1"/>
  </g>
  
  <!-- User Interface Technologies -->
  <g transform="translate(390, 420)" filter="url(#shadow)">
    <circle cx="0" cy="0" r="45" fill="#34495e"/>
    <text x="0" y="-15" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">User Interface</text>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">Technologies</text>
    <text x="0" y="15" font-family="Arial, sans-serif" font-size="11" fill="#ffffff" text-anchor="middle">AR/VR/Mobile</text>
    
    <!-- Devices icons -->
    <rect x="-20" y="-25" width="15" height="10" rx="1" fill="#ffffff"/> <!-- Computer -->
    <rect x="-25" y="-15" width="25" height="15" rx="1" fill="#ffffff"/> <!-- Base -->
    
    <rect x="5" y="-30" width="10" height="20" rx="1" fill="#ffffff"/> <!-- Phone -->
    <circle cx="10" cy="-20" r="2" fill="#34495e"/> <!-- Button -->
    
    <rect x="20" y="-25" width="15" height="10" rx="5" fill="#ffffff"/> <!-- VR Headset -->
    <rect x="20" y="-25" width="15" height="10" rx="5" stroke="#34495e" stroke-width="1" fill="none"/>
    <line x1="27.5" y1="-25" x2="27.5" y2="-35" stroke="#ffffff" stroke-width="1"/>
  </g>
  
  <!-- Connection lines to center -->
  <g stroke="#2c3e50" stroke-width="1.5" stroke-dasharray="3,3">
    <line x1="300" y1="145" x2="300" y2="180"/>
    <line x1="155" y1="180" x2="240" y2="215"/>
    <line x1="445" y1="180" x2="360" y2="215"/>
    <line x1="155" y1="320" x2="240" y2="285"/>
    <line x1="445" y1="320" x2="360" y2="285"/>
    <line x1="210" y1="375" x2="260" y2="320"/>
    <line x1="390" y1="375" x2="340" y2="320"/>
  </g>
  
  <!-- Bottom caption -->
  <g transform="translate(300, 480)">
    <rect x="-290" y="-15" width="580" height="30" rx="15" fill="#2c3e50" opacity="0.7"/>
    <text x="0" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">
      Based on Ucaretron Inc. Patented Technology
    </text>
  </g>
</svg>