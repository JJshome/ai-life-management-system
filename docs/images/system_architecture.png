<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background elements -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a8f" stop-opacity="0.2"/>
      <stop offset="100%" stop-color="#42b0d1" stop-opacity="0.2"/>
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="3" dy="3" stdDeviation="5" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Decorative background -->
  <rect x="0" y="0" width="800" height="600" fill="url(#bgGradient)"/>
  <circle cx="400" cy="300" r="280" fill="none" stroke="#5a8de3" stroke-width="1.5" stroke-dasharray="8,8"/>
  
  <!-- System Core -->
  <g transform="translate(400, 300)" filter="url(#shadow)">
    <circle cx="0" cy="0" r="80" fill="#2c3e50" stroke="#3498db" stroke-width="3"/>
    <text x="0" y="-10" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle" dominant-baseline="middle">AI Life</text>
    <text x="0" y="10" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle" dominant-baseline="middle">Management</text>
    <text x="0" y="30" font-family="Arial, sans-serif" font-size="12" fill="#ffffff" text-anchor="middle" dominant-baseline="middle">System Core</text>
  </g>
  
  <!-- Module 110: Data Collection -->
  <g transform="translate(275, 150)" filter="url(#shadow)">
    <rect x="-75" y="-45" width="150" height="90" rx="10" fill="#3498db" stroke="#2980b9" stroke-width="2"/>
    <text x="0" y="-20" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" font-weight="bold" text-anchor="middle">Data Collection</text>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Module (110)</text>
    
    <!-- Small cartoon person with sensors -->
    <g transform="translate(-40, 25) scale(0.4)">
      <circle cx="0" cy="0" r="20" fill="#ffffff"/> <!-- Head -->
      <circle cx="-7" cy="-5" r="3" fill="#2980b9"/> <!-- Eye -->
      <circle cx="7" cy="-5" r="3" fill="#2980b9"/> <!-- Eye -->
      <path d="M-7,5 Q0,10 7,5" stroke="#2980b9" stroke-width="2" fill="none"/> <!-- Smile -->
      <rect x="-15" y="20" width="30" height="40" rx="5" fill="#ffffff"/> <!-- Body -->
      <line x1="-15" y1="30" x2="-30" y2="15" stroke="#ffffff" stroke-width="6" stroke-linecap="round"/> <!-- Arm -->
      <line x1="15" y1="30" x2="30" y2="15" stroke="#ffffff" stroke-width="6" stroke-linecap="round"/> <!-- Arm -->
      
      <!-- Sensors -->
      <circle cx="-25" cy="0" r="8" fill="#e74c3c"/> <!-- Wearable -->
      <rect x="20" y="-20" width="15" height="10" rx="2" fill="#f1c40f"/> <!-- Sensor -->
      <circle cx="0" cy="-30" r="5" fill="#2ecc71"/> <!-- Ear sensor -->
    </g>
    
    <!-- Data streams -->
    <path d="M0,25 m-25,-10 l10,10 l10,-5 l10,5 l10,-10 l10,10" stroke="#ffffff" stroke-width="2" fill="none"/>
  </g>
  
  <!-- Module 120: AI Analysis -->
  <g transform="translate(525, 150)" filter="url(#shadow)">
    <rect x="-75" y="-45" width="150" height="90" rx="10" fill="#9b59b6" stroke="#8e44ad" stroke-width="2"/>
    <text x="0" y="-20" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" font-weight="bold" text-anchor="middle">AI Analysis</text>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Module (120)</text>
    
    <!-- AI cartoon brain with processing -->
    <g transform="translate(0, 20) scale(0.4)">
      <path d="M-30,-20 Q-40,-40 -20,-45 Q0,-50 20,-45 Q40,-40 30,-20 Q40,0 30,20 Q40,40 20,45 Q0,50 -20,45 Q-40,40 -30,20 Q-40,0 -30,-20" 
        fill="#ffffff" stroke="#8e44ad" stroke-width="3"/>
      
      <!-- Synapses and connections -->
      <circle cx="-10" cy="-20" r="3" fill="#8e44ad"/>
      <circle cx="10" cy="-30" r="3" fill="#8e44ad"/>
      <circle cx="20" cy="-10" r="3" fill="#8e44ad"/>
      <circle cx="15" cy="15" r="3" fill="#8e44ad"/>
      <circle cx="-15" cy="25" r="3" fill="#8e44ad"/>
      <circle cx="-20" cy="5" r="3" fill="#8e44ad"/>
      
      <line x1="-10" y1="-20" x2="10" y2="-30" stroke="#8e44ad" stroke-width="1.5"/>
      <line x1="10" y1="-30" x2="20" y2="-10" stroke="#8e44ad" stroke-width="1.5"/>
      <line x1="20" y1="-10" x2="15" y2="15" stroke="#8e44ad" stroke-width="1.5"/>
      <line x1="15" y1="15" x2="-15" y2="25" stroke="#8e44ad" stroke-width="1.5"/>
      <line x1="-15" y1="25" x2="-20" y2="5" stroke="#8e44ad" stroke-width="1.5"/>
      <line x1="-20" y1="5" x2="-10" y2="-20" stroke="#8e44ad" stroke-width="1.5"/>
    </g>
  </g>
  
  <!-- Module 130: Prediction Engine -->
  <g transform="translate(650, 300)" filter="url(#shadow)">
    <rect x="-75" y="-45" width="150" height="90" rx="10" fill="#e74c3c" stroke="#c0392b" stroke-width="2"/>
    <text x="0" y="-20" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" font-weight="bold" text-anchor="middle">Prediction</text>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Module (130)</text>
    
    <!-- Crystal ball prediction -->
    <g transform="translate(0, 20) scale(0.5)">
      <!-- Stand -->
      <path d="M-20,30 L20,30 L15,50 L-15,50 Z" fill="#ffffff"/>
      
      <!-- Ball -->
      <circle cx="0" cy="0" r="30" fill="#ffffff" opacity="0.7"/>
      <circle cx="0" cy="0" r="25" fill="none" stroke="#ffffff" stroke-width="1"/>
      
      <!-- Future path visualization -->
      <polyline points="-15,10 -10,0 -5,5 0,-10 5,-5 10,0 15,-10" 
        stroke="#c0392b" stroke-width="2" fill="none"/>
    </g>
  </g>
  
  <!-- Module 140: Monitoring -->
  <g transform="translate(525, 450)" filter="url(#shadow)">
    <rect x="-75" y="-45" width="150" height="90" rx="10" fill="#2ecc71" stroke="#27ae60" stroke-width="2"/>
    <text x="0" y="-20" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" font-weight="bold" text-anchor="middle">Monitoring</text>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Module (140)</text>
    
    <!-- Health monitoring cartoon -->
    <g transform="translate(0, 20) scale(0.5)">
      <!-- Monitor frame -->
      <rect x="-30" y="-20" width="60" height="40" rx="5" fill="#ffffff"/>
      
      <!-- Screen elements -->
      <polyline points="-20,-10 -10,0 -5,-5 0,10 5,5 10,0 20,-5" 
        stroke="#27ae60" stroke-width="2" fill="none"/>
      
      <!-- Heart monitor blip -->
      <polyline points="-20,15 -15,15 -10,15 -5,0 0,20 5,15 10,15 15,15 20,15" 
        stroke="#e74c3c" stroke-width="2" fill="none"/>
    </g>
  </g>
  
  <!-- Module 150: Security -->
  <g transform="translate(275, 450)" filter="url(#shadow)">
    <rect x="-75" y="-45" width="150" height="90" rx="10" fill="#f39c12" stroke="#d35400" stroke-width="2"/>
    <text x="0" y="-20" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" font-weight="bold" text-anchor="middle">Security</text>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Module (150)</text>
    
    <!-- Security shield cartoon -->
    <g transform="translate(0, 20) scale(0.5)">
      <!-- Shield -->
      <path d="M0,-30 L25,-20 Q30,10 0,30 Q-30,10 -25,-20 Z" fill="#ffffff"/>
      
      <!-- Lock -->
      <rect x="-10" y="-5" width="20" height="20" rx="2" fill="#d35400"/>
      <circle cx="0" cy="-10" r="10" fill="none" stroke="#d35400" stroke-width="3"/>
      <rect x="-2" y="-5" width="4" height="10" fill="#d35400"/>
    </g>
  </g>
  
  <!-- Module 160: User Interface -->
  <g transform="translate(150, 300)" filter="url(#shadow)">
    <rect x="-75" y="-45" width="150" height="90" rx="10" fill="#1abc9c" stroke="#16a085" stroke-width="2"/>
    <text x="0" y="-20" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" font-weight="bold" text-anchor="middle">User Interface</text>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Module (160)</text>
    
    <!-- UI cartoon elements -->
    <g transform="translate(0, 20) scale(0.5)">
      <!-- Device -->
      <rect x="-25" y="-30" width="50" height="70" rx="5" fill="#ffffff"/>
      
      <!-- Screen elements -->
      <rect x="-20" y="-25" width="40" height="10" rx="2" fill="#16a085"/>
      <rect x="-20" y="-10" width="15" height="15" rx="2" fill="#3498db"/>
      <rect x="0" y="-10" width="20" height="15" rx="2" fill="#9b59b6"/>
      <rect x="-20" y="10" width="40" height="10" rx="2" fill="#e74c3c"/>
      <rect x="-20" y="25" width="25" height="10" rx="2" fill="#f1c40f"/>
    </g>
  </g>
  
  <!-- Connection lines -->
  <g stroke="#ffffff" stroke-width="2.5" stroke-dasharray="5,5" filter="url(#shadow)">
    <!-- Data Collection to Core -->
    <line x1="325" y1="200" x2="370" y2="255"/>
    
    <!-- AI Analysis to Core -->
    <line x1="475" y1="200" x2="430" y2="255"/>
    
    <!-- Prediction to Core -->
    <line x1="575" y1="300" x2="480" y2="300"/>
    
    <!-- Monitoring to Core -->
    <line x1="475" y1="400" x2="430" y2="345"/>
    
    <!-- Security to Core -->
    <line x1="325" y1="400" x2="370" y2="345"/>
    
    <!-- User Interface to Core -->
    <line x1="225" y1="300" x2="320" y2="300"/>
  </g>
  
  <!-- Ucaretron Badge -->
  <g transform="translate(400, 560)" filter="url(#shadow)">
    <rect x="-200" y="-25" width="400" height="50" rx="25" fill="#2c3e50" stroke="#3498db" stroke-width="2"/>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle" dominant-baseline="middle">Powered by Ucaretron Inc. Patented Technology</text>
    <g transform="translate(-170, 0)">
      <circle cx="0" cy="0" r="15" fill="#3498db"/>
      <path d="M-5,-5 L0,-12 L5,-5 L10,0 L5,5 L0,12 L-5,5 L-10,0 Z" fill="#ffffff"/>
    </g>
  </g>
</svg>