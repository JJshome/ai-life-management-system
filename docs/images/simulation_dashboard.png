<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1a3a8f" stop-opacity="0.1"/>
      <stop offset="100%" stop-color="#42b0d1" stop-opacity="0.1"/>
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="2" dy="2" stdDeviation="3" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Background rectangle -->
  <rect x="0" y="0" width="800" height="600" fill="url(#bg-gradient)" rx="10" ry="10"/>
  
  <!-- Application header -->
  <rect x="0" y="0" width="800" height="60" fill="#2c3e50"/>
  <text x="30" y="38" font-family="Arial, sans-serif" font-size="24" fill="#ffffff">AI Life Management System - Simulation</text>
  
  <!-- Left sidebar -->
  <rect x="0" y="60" width="200" height="540" fill="#34495e"/>
  
  <!-- Menu items -->
  <rect x="10" y="80" width="180" height="40" rx="5" fill="#3498db"/>
  <text x="100" y="105" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Dashboard</text>
  
  <rect x="10" y="130" width="180" height="40" rx="5" fill="#2c3e50" stroke="#3498db" stroke-width="1"/>
  <text x="100" y="155" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Health Data</text>
  
  <rect x="10" y="180" width="180" height="40" rx="5" fill="#2c3e50" stroke="#3498db" stroke-width="1"/>
  <text x="100" y="205" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Predictions</text>
  
  <rect x="10" y="230" width="180" height="40" rx="5" fill="#2c3e50" stroke="#3498db" stroke-width="1"/>
  <text x="100" y="255" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Recommendations</text>
  
  <rect x="10" y="280" width="180" height="40" rx="5" fill="#2c3e50" stroke="#3498db" stroke-width="1"/>
  <text x="100" y="305" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Scenarios</text>
  
  <rect x="10" y="330" width="180" height="40" rx="5" fill="#2c3e50" stroke="#3498db" stroke-width="1"/>
  <text x="100" y="355" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Settings</text>
  
  <!-- User profile -->
  <rect x="10" y="500" width="180" height="80" rx="5" fill="#2c3e50" stroke="#3498db" stroke-width="1"/>
  <circle cx="45" cy="530" r="25" fill="#3498db"/>
  <text x="45" y="535" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">JD</text>
  <text x="120" y="520" font-family="Arial, sans-serif" font-size="14" fill="#ffffff">John Doe</text>
  <text x="120" y="540" font-family="Arial, sans-serif" font-size="12" fill="#bdc3c7">Age: 45</text>
  <text x="120" y="555" font-family="Arial, sans-serif" font-size="12" fill="#bdc3c7">Profile #1</text>
  
  <!-- Main content area -->
  <rect x="200" y="60" width="600" height="540" fill="#ecf0f1"/>
  
  <!-- Dashboard section title -->
  <text x="230" y="100" font-family="Arial, sans-serif" font-size="24" fill="#2c3e50">Simulation Dashboard</text>
  
  <!-- Life expectancy card -->
  <g transform="translate(300, 170)" filter="url(#shadow)">
    <rect x="-90" y="-60" width="180" height="120" rx="10" fill="#ffffff" stroke="#3498db" stroke-width="2"/>
    <text x="0" y="-35" font-family="Arial, sans-serif" font-size="16" fill="#2c3e50" text-anchor="middle">Life Expectancy</text>
    <text x="0" y="10" font-family="Arial, sans-serif" font-size="36" fill="#3498db" text-anchor="middle">89.4</text>
    <text x="0" y="35" font-family="Arial, sans-serif" font-size="16" fill="#7f8c8d" text-anchor="middle">years</text>
  </g>
  
  <!-- Biological age card -->
  <g transform="translate(500, 170)" filter="url(#shadow)">
    <rect x="-90" y="-60" width="180" height="120" rx="10" fill="#ffffff" stroke="#e74c3c" stroke-width="2"/>
    <text x="0" y="-35" font-family="Arial, sans-serif" font-size="16" fill="#2c3e50" text-anchor="middle">Biological Age</text>
    <text x="0" y="10" font-family="Arial, sans-serif" font-size="36" fill="#e74c3c" text-anchor="middle">40.2</text>
    <text x="0" y="35" font-family="Arial, sans-serif" font-size="16" fill="#7f8c8d" text-anchor="middle">vs. 45 chronological</text>
  </g>
  
  <!-- Health score card -->
  <g transform="translate(700, 170)" filter="url(#shadow)">
    <rect x="-90" y="-60" width="180" height="120" rx="10" fill="#ffffff" stroke="#2ecc71" stroke-width="2"/>
    <text x="0" y="-35" font-family="Arial, sans-serif" font-size="16" fill="#2c3e50" text-anchor="middle">Health Score</text>
    <text x="0" y="10" font-family="Arial, sans-serif" font-size="36" fill="#2ecc71" text-anchor="middle">86</text>
    <text x="0" y="35" font-family="Arial, sans-serif" font-size="16" fill="#7f8c8d" text-anchor="middle">out of 100</text>
  </g>
  
  <!-- Risk factors chart -->
  <g transform="translate(350, 330)" filter="url(#shadow)">
    <rect x="-140" y="-60" width="280" height="200" rx="10" fill="#ffffff"/>
    <text x="0" y="-35" font-family="Arial, sans-serif" font-size="16" fill="#2c3e50" text-anchor="middle">Health Risk Factors</text>
    
    <!-- Risk bar 1 -->
    <text x="-120" y="-10" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50">Cardiovascular</text>
    <rect x="-120" y="0" width="200" height="15" rx="7.5" fill="#ecf0f1"/>
    <rect x="-120" y="0" width="70" height="15" rx="7.5" fill="#f1c40f"/>
    
    <!-- Risk bar 2 -->
    <text x="-120" y="30" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50">Metabolic</text>
    <rect x="-120" y="40" width="200" height="15" rx="7.5" fill="#ecf0f1"/>
    <rect x="-120" y="40" width="40" height="15" rx="7.5" fill="#2ecc71"/>
    
    <!-- Risk bar 3 -->
    <text x="-120" y="70" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50">Neurological</text>
    <rect x="-120" y="80" width="200" height="15" rx="7.5" fill="#ecf0f1"/>
    <rect x="-120" y="80" width="30" height="15" rx="7.5" fill="#2ecc71"/>
    
    <!-- Risk bar 4 -->
    <text x="-120" y="110" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50">Cancer</text>
    <rect x="-120" y="120" width="200" height="15" rx="7.5" fill="#ecf0f1"/>
    <rect x="-120" y="120" width="50" height="15" rx="7.5" fill="#2ecc71"/>
  </g>
  
  <!-- Lifestyle impact chart -->
  <g transform="translate(650, 330)" filter="url(#shadow)">
    <rect x="-140" y="-60" width="280" height="200" rx="10" fill="#ffffff"/>
    <text x="0" y="-35" font-family="Arial, sans-serif" font-size="16" fill="#2c3e50" text-anchor="middle">Lifestyle Impact</text>
    
    <!-- Exercise impact -->
    <g transform="translate(0, 0)">
      <circle cx="-100" cy="0" r="15" fill="#3498db"/>
      <text x="-100" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">E</text>
      <text x="-70" y="5" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50">Exercise</text>
      <text x="100" y="5" font-family="Arial, sans-serif" font-size="14" fill="#3498db" text-anchor="end">+4.2 years</text>
    </g>
    
    <!-- Nutrition impact -->
    <g transform="translate(0, 40)">
      <circle cx="-100" cy="0" r="15" fill="#2ecc71"/>
      <text x="-100" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">N</text>
      <text x="-70" y="5" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50">Nutrition</text>
      <text x="100" y="5" font-family="Arial, sans-serif" font-size="14" fill="#2ecc71" text-anchor="end">+3.8 years</text>
    </g>
    
    <!-- Sleep impact -->
    <g transform="translate(0, 80)">
      <circle cx="-100" cy="0" r="15" fill="#9b59b6"/>
      <text x="-100" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">S</text>
      <text x="-70" y="5" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50">Sleep</text>
      <text x="100" y="5" font-family="Arial, sans-serif" font-size="14" fill="#9b59b6" text-anchor="end">+2.5 years</text>
    </g>
    
    <!-- Stress impact -->
    <g transform="translate(0, 120)">
      <circle cx="-100" cy="0" r="15" fill="#f39c12"/>
      <text x="-100" y="5" font-family="Arial, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">S</text>
      <text x="-70" y="5" font-family="Arial, sans-serif" font-size="14" fill="#2c3e50">Stress Management</text>
      <text x="100" y="5" font-family="Arial, sans-serif" font-size="14" fill="#f39c12" text-anchor="end">+1.9 years</text>
    </g>
  </g>
  
  <!-- Action buttons -->
  <rect x="230" y="530" width="160" height="40" rx="5" fill="#3498db" filter="url(#shadow)"/>
  <text x="310" y="555" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Run Simulation</text>
  
  <rect x="410" y="530" width="160" height="40" rx="5" fill="#2ecc71" filter="url(#shadow)"/>
  <text x="490" y="555" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Test Scenarios</text>
  
  <rect x="590" y="530" width="160" height="40" rx="5" fill="#e74c3c" filter="url(#shadow)"/>
  <text x="670" y="555" font-family="Arial, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Export Results</text>
  
  <!-- Status bar -->
  <rect x="0" y="580" width="800" height="20" fill="#2c3e50"/>
  <text x="780" y="594" font-family="Arial, sans-serif" font-size="12" fill="#ffffff" text-anchor="end">
    Based on Ucaretron Inc. Patented Technology
  </text>
</svg>